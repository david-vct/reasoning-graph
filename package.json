{
  "name": "reasoning-graph",
  "version": "1.0.0",
  "private": true,
  "description": "Visual reasoning graph tool with logical validation",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "npm run dev --workspace=apps/web",
    "build": "npm run build --workspaces",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "npm run test:e2e --workspace=apps/web",
    "lint": "npm run lint --workspaces",
    "type-check": "tsc --noEmit",
    "check": "npm run lint && npm run type-check && npm run build && npm test",
    "prepare": "husky || true"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md,yml}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/geojson": "^7946.0.16",
    "@types/jest": "^30.0.0",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^25.0.9",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.0",
    "ts-jest": "^29.4.6"
  },
  "keywords": [
    "reasoning",
    "graph",
    "logic",
    "validation"
  ],
  "author": "",
  "license": "MIT"
}
